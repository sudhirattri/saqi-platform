###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#hasObservation
SAQIOntology:hasObservation rdf:type owl:ObjectProperty ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( SAQIOntology:Pollutant
                                                        SAQIOntology:PrescribedStandardForPollutant
                                                      )
                                        ] ;
                            rdfs:range SAQIOntology:Observation ;
                            cpannotationschema:reengineeredFrom "Observation ODP - http://ontologydesignpatterns.org/wiki/Submissions:Observation" .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#hasPoint
SAQIOntology:hasPoint rdf:type owl:ObjectProperty ;
                      rdfs:domain SAQIOntology:Trajectory ;
                      rdfs:range SAQIOntology:TrajectoryPoint ;
                      owl:propertyChainAxiom ( SAQIOntology:hasSegment
                                               SAQIOntology:endsAt
                                             ) ,
                                             ( SAQIOntology:hasSegment
                                               SAQIOntology:startsFrom
                                             ) ;
                      cpannotationschema:reengineeredFrom "Trajectory ODP - http://ontologydesignpatterns.org/wiki/Submissions:Trajectory" ;
                      rdfs:comment "Relating the trajectory to each of its points." .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#hasPrescribedStandard
SAQIOntology:hasPrescribedStandard rdf:type owl:ObjectProperty ;
                                   rdfs:domain SAQIOntology:Pollutant ,
                                               SAQIOntology:PrescribedStandardForPollutant ;
                                   rdfs:range SAQIOntology:PrescribedStandardForPollutant .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#hasSchemes
SAQIOntology:hasSchemes rdf:type owl:ObjectProperty ;
