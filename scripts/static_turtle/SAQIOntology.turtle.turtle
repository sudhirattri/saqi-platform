                                                    SAQIOntology:Authors "Saad Ahmad, Sudhir Attri, Raghava Mutharaju" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
terms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
terms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#Authors
SAQIOntology:Authors rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#actsFor
SAQIOntology:actsFor rdf:type owl:ObjectProperty ;
                     rdfs:domain SAQIOntology:Person ;
                     rdfs:range SAQIOntology:Institution .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#actsThrough
SAQIOntology:actsThrough rdf:type owl:ObjectProperty ;
                         rdfs:domain SAQIOntology:Institution ;
                         rdfs:range SAQIOntology:Person .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#atPlace
SAQIOntology:atPlace rdf:type owl:ObjectProperty ;
                     owl:equivalentProperty SAQIOntology:livesIn ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:range SAQIOntology:Place .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#carriesPollutant
SAQIOntology:carriesPollutant rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain SAQIOntology:Carrier ;
                              rdfs:range SAQIOntology:Pollutant ;
                              cpannotationschema:reengineeredFrom "Trajectory ODP - http://ontologydesignpatterns.org/wiki/Submissions:Trajectory" .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#endsAt
SAQIOntology:endsAt rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain SAQIOntology:TrajectorySegment ;
                    rdfs:range SAQIOntology:TrajectoryPoint ;
                    cpannotationschema:reengineeredFrom "Trajectory ODP - http://ontologydesignpatterns.org/wiki/Submissions:Trajectory" ;
                    rdfs:comment "Connects a segment to the point it ends at." .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#givesInformationTo
SAQIOntology:givesInformationTo rdf:type owl:ObjectProperty ;
                                rdfs:domain [ rdf:type owl:Class ;
                                              owl:unionOf ( SAQIOntology:Government
                                                            SAQIOntology:Institution
                                                          )
                                            ] ;
                                rdfs:range SAQIOntology:Person .


###  https://kracr.iiitd.edu.in/ontology/SAQIOntology#hasAirPollutionLiteracy
